<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.test.viewdemo.viewPager.viewpagerListener.customSlidingTabLayout.MyTabLayoutActivity">

    <com.test.viewdemo.viewPager.viewpagerListener.customSlidingTabLayout.mySlindingTabLayout3.MySlindingTabLayout
        android:id="@+id/horizontal_scrollView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></com.test.viewdemo.viewPager.viewpagerListener.customSlidingTabLayout.mySlindingTabLayout3.MySlindingTabLayout>

    <com.test.viewdemo.viewPager.viewpagerListener.customSlidingTabLayout.mySlindingTabLayout4.MySlindingTabLayout
        android:id="@+id/tab"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <android.support.v4.view.ViewPager
        android:id="@+id/view_page"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></android.support.v4.view.ViewPager>
</LinearLayout>
